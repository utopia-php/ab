{
    "name": "utopia-php/ab",
    "description": "A simple AB Test library to manage server side AB testing",
    "type": "library",
    "keywords": ["php","framework", "upf", "utopia", "ab", "ab test", "ab testing", "ab tests"],
    "license": "MIT",
    "autoload": {
        "psr-4": {"Utopia\\AB\\": "src/AB"}
    },
    "scripts": {
        "lint": "./vendor/bin/pint --test",
        "format": "./vendor/bin/pint",
        "test": "./vendor/bin/phpunit --configuration phpunit.xml"
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "4.0.1",
        "laravel/pint": "1.2.*"
    },
    "minimum-stability": "dev"
}
